
<!DOCTYPE html>
<html>
<head>
    @Styles.Render("~/Content/themes/base/css")
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/js")
    @Scripts.Render("~/bundles/jqueryui")
    @RenderSection("scripts", required: false)
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
    <script type="text/javascript">$(document).ready(function () { datepickize('.inputDate'); });</script>
</head>
<body>
    @Html.Hidden("rootPath", HttpRuntime.AppDomainAppVirtualPath, new { id = "rootPath" })
    @RenderBody()
    <div style="text-align:right">

        <button title="Close" style="right: 20px;" onclick="window.close()"><i class="fas fa-window-close"></i> Close</button>

    </div>
 </body>
</html>