var ctxPath = "/SMM";

function submitToPage(formName, relActionPath, method){
    var form = document.getElementById(formName);
    form.action =
        '/SMM/' + relActionPath + '.do?method=' + method;
    form.submit();
}
function confirmSubmitToPage(formName, relActionPath, method, msg){
    if(confirm(msg)){
        submitToPage(formName, relActionPath, method);
        return true;
    }else
        return false;
}
function goToPage(relActionPath, method, paramStr){
    var url = '/SMM/' + relActionPath +
        '.do?method=' + method;
    if(typeof paramStr != "undefined")
        url += '&' + paramStr;
    window.location.replace(url);
}
function confirmGoToPage(relActionPath, method, paramStr, msg){
    if(confirm(msg)){
        window.location.replace('/SMM/' + relActionPath +
            '.do?method=' + method + '&' + paramStr);
        return true;
    }else
        return false;
}
function disableAllBtns(){
    $("input:button").each(function(){
        $(this).attr("disabled", "disabled");
    });
}
function enableAllBtns(){
    $("input:button").each(function(){
        $(this).removeAttr("disabled");
    });
}
function selectSignboard(signboardNoID, streetNameID, streetNoID, buildingNameID, bcisBlockId, addRefNo){
    disableAllBtns();
    var url = ctxPath + "/signboardSelector.do?method=show";
    var signboard = popup(url, null,
        "dialogWidth:1024px; dialogHeight:768px; center:yes");
    if(typeof signboard != "undefined" && signboard != null){
        if(typeof signboardNoID != "undefined")
            document.getElementById(signboardNoID).value = signboard.signboardNo;
        if(typeof streetNameID != "undefined" && typeof streetNoID != "undefined" &&
                typeof buildingNameID != "undefined"){
            var streetNameObj = document.getElementById(streetNameID);
            var streetNoObj = document.getElementById(streetNoID);
            var buildingNameObj = document.getElementById(buildingNameID);
            if(streetNameObj.value == "" && streetNoObj.value == "" &&
                    buildingNameObj.value == ""){
                streetNameObj.value = signboard.streetName;
                streetNoObj.value = signboard.streetNo;
                buildingNameObj.value = signboard.buildingName;
            }
        }
        if(typeof bcisBlockId != "undefined" && typeof addRefNo != "undefined"){
        	var bcisBlockIdObj = document.getElementById(bcisBlockId);
        	var addRefNoObj = document.getElementById(addRefNo);
        	if(bcisBlockIdObj.value == "" && addRefNoObj.value == ""){
                bcisBlockIdObj.value = signboard.bcisBlockId;
                addRefNoObj.value = signboard.addRefNo;

            }
        }
    }
    enableAllBtns();
    if(typeof signboard != "undefined" && signboard != null)
        return signboard.signboardNo;
}

function selectReport(reportNo){
    disableAllBtns();
    var url = ctxPath + "/signboardSelector.do?method=showReport";
    var report = popup(url, null,
        "dialogWidth:1024px; dialogHeight:768px; center:yes");
    if(typeof report != "undefined" && report != null){
        if(typeof reportNo != "undefined")
            document.getElementById(reportNo).value = report.reportNo;
    }
    enableAllBtns();
    if(typeof report != "undefined" && report != null)
        return report.reportNo;
}

function selectS24(s24No){
    disableAllBtns();
    var url = ctxPath + "/signboardSelector.do?method=showS24";
    var s24 = popup(url, null,
        "dialogWidth:1024px; dialogHeight:768px; center:yes");
    if(typeof s24 != "undefined" && s24 != null){
        if(typeof s24No != "undefined")
            document.getElementById(s24No).value = s24.s24OrderNo;
    }
    enableAllBtns();
    if(typeof s24 != "undefined" && s24 != null)
        return s24.s24OrderNo;
}

function toStr(strArray){
    var str = "";
    for(var a in strArray){
        if(a > 0) str += ", ";
        str += strArray[a];
    }
    return str;
}
function popup(url, param, options){
    url += "&time=" + (new Date());
    var returnVal = window.showModalDialog(url, param, options);
    return returnVal;
}
function jq_escape(str){
    return str.replace("[", "\\[").replace("]", "\\]").replace(".", "\\.");
}
function regExp_escape(str){
    return str.replace(/[-[\]{}()*+?.,\\^$|#\s]/g, "\$&");
}
