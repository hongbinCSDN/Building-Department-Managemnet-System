                        <script>
var displayChecked = "on";
var displayUnchecked = "";
var trueStr = "Y";
var falseStr = "N";
var okDisplay = "OK";
var notOkDisplay = "Not OK";
var okNotOkDisplay = "OK / Not OK";


function getParamStr(){
    var param = '&sessionID=d24cdfb0-b45e-4d69-9a60-70186e5ad25d';
    var editMode = document.ValidationTaskForm.editMode.value;
    var isPopup = document.ValidationTaskForm.isPopup.value;
    if(editMode == "view")
        param += "&editMode=" + editMode;
    if(isPopup == "Y")
        param += "&isPopup=" + isPopup;
    return param;
}
function submitToDo(page){
    disableAllBtns();
    submitToPage('validationTaskForm', 'task/validation', page + getParamStr());
}
function confirmSubmitToDo(page, msg){
    disableAllBtns();
    if(!confirmSubmitToPage('validationTaskForm', 'task/validation',
            page + getParamStr(), msg))
       enableAllBtns();
}
function enableSubmit(){
    $("#btnSubmit").removeAttr("disabled");
}
function blockRadioBtnSelect(radioObj){
    var name = radioObj.name;
    var jObj = $("input:hidden[name=" + name + "]");
    $("input:radio[name=" + name + "]").each(function(){
        if($(this).val() == jObj.val())
            $(this).attr("checked", "checked");
    });
}
function blockDropDownChange(selectObj){
    var name = selectObj.name;
    var jObj = $("input:hidden[name=" + name + "]");
    $("select[name=" + name + "]").val(jObj.val());
}
function toggleOkBtn(btnObj){
    if(btnObj.name != "")
        var fieldObj = document.getElementById(btnObj.name);
    if(typeof fieldObj != "undefined"){
        if(fieldObj.value == displayChecked)
            fieldObj.value = displayUnchecked;
        else if(fieldObj.value == displayUnchecked)
            fieldObj.value = displayChecked;
    }
    if(btnObj.value == okDisplay)
        btnObj.value = notOkDisplay;
    else if(btnObj.value == notOkDisplay)
        btnObj.value = okDisplay;
}
function toggleOkNullBtn(btnObj){
    if(btnObj.name != "")
        var fieldObj = document.getElementById(btnObj.name);
    if(typeof fieldObj != "undefined"){
        if(fieldObj.value == trueStr)
            fieldObj.value = falseStr;
        else if(fieldObj.value == falseStr)
            fieldObj.value = "";
        else if(fieldObj.value == "")
            fieldObj.value = trueStr;
    }
    if(btnObj.value == okDisplay)
        btnObj.value = notOkDisplay;
    else if(btnObj.value == notOkDisplay)
        btnObj.value = okNotOkDisplay;
    else if(btnObj.value == okNotOkDisplay)
        btnObj.value = okDisplay;
}
function setOkBtn(btnJObj, isChecked, isOk){
    var fieldObj = document.getElementById(btnJObj.attr("name"));

    if(typeof fieldObj != "undefined" && fieldObj != null){
      fieldObj.value = isChecked;
      btnJObj.val(isOk);
    }
}
