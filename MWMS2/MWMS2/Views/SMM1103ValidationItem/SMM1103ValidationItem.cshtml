@model IEnumerable<MWMS2.Entity.B_S_SYSTEM_VALUE>

<h3>Validation Item</h3>
<div class="btnSet btnSetTypeA">
    <div class="right">
        <input class="inputButton" id="" name="" value="Create Validation Item" onclick='location.href="@Url.Action("SMM1103ValidationItemCreate", "SMM1103ValidationItem")"' type="button">
    </div>
</div>
<br>
<div class="sList">
    <table class="resultTable">
        <thead>
            <tr>
                <th class="sTh">Validation Item</th>
                <th class="sTh">Corresponding MW Item</th>
                <th class="sTh">Ordering</th>


            </tr>
        </thead>
        <tbody>
            @{
                foreach (var item in Model)
                {
                    <tr>
                        <td>

                            @Html.ActionLink(item.CODE, "SMM1103ValidationItemEdit", "SMM1103ValidationItem", new { id = item.UUID }, null)
                            @*@Html.DisplayFor(modelItem => item.REGISTRATION_NO)*@
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DESCRIPTION)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ORDERING)
                        </td>
                
                    </tr>
                }
            }
        </tbody>
    </table>
</div>
<br>
