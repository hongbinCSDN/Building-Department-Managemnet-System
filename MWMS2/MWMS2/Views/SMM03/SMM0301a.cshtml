                      remarks.eq(i).hide();
                  }
            });
        }
    }
}
function explode() {
    $(".remarks").each(function(){
        $(this).show();
    });
}
function showButtonClick(sender) {
    if(sender.value == "Show All") { 
        explode();
        sender.value = "Show \"Not OK\" Only";
    } else {
        implode();
        sender.value = "Show All";
    }
}
