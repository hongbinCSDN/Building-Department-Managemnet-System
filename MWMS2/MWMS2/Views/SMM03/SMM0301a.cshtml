<script type="text/javascript">
function implode() { 
    var tempRow1 = null;
    var remarks = $(".remarks");
    for(var i = 0 ; i < remarks.length; i++){
        if(remarks.eq(i).attr("class").match("sRow1") != null)
            tempRow1 = remarks.eq(i);
        else if (remarks.eq(i).attr("class").match("sRow2") != null){
            var ind = (i - 1) / 4;
            if(i > 11){
                tempRow1 = null;
                if(i == 12 || i == 13 || i == 16)
                    ind = 3;
                else if(i == 14 || i == 15)
                    ind = 4;
            }
            $(".results").eq(ind).find("input:hidden").each(function(){
                if(/*$(this).attr('checked') &&*/ 
