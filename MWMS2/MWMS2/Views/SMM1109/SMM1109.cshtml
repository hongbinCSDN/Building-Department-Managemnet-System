@model IEnumerable<MWMS2.Entity.B_S_SYSTEM_VALUE>

<h3>
    Types of Signboard
</h3>

<div class="btnSet btnSetTypeA">
    <div class="right">
        <input class="inputButton" id="" name="" value="Add" onclick='location.href="@Url.Action("SMM1109Create", "SMM1109")"' type="button">
    </div>
</div>
<br>

<div style="overflow-x:auto">
    <table class="resultTable">
        <thead>
            <tr>
                <th>

                </th>
                <th class="sTh">
                    No.
                </th>
                <th class="sTh">
                    Type Of Signboard
                </th>
                <th class="sTh">
                    Description
                </th>


            </tr>
        </thead>
        <tbody>
            @{
                foreach (var item in Model)
                {
                    <tr>
                        <td>
                            <input class="inputButton" id="" name="" value="Delete" onclick='location.href="@Url.Action("SMM1109Delete", "SMM1109",new { id=item.UUID })"' type="button">

                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.ORDERING)
                        </td>
                        <td>

                            @Html.ActionLink(item.CODE, "SMM1109Edit", "SMM1109", new { id = item.UUID }, null)
                            @*@Html.DisplayFor(modelItem => item.REGISTRATION_NO)*@
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DESCRIPTION)
                        </td>


                    </tr>
                }
            }
        </tbody>
    </table>


</div>
