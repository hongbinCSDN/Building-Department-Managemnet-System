    function triggerExport(certNo, exportType) {
        var selectedProcess = document.getElementById('process').value;
        var indCertUuid = document.getElementById('C_IND_CERTIFICATE_UUID').value;
        var issueDt = "";
        var lastDocRecDt = "";
        var signature = "";

        // base on selectedProcess, get different input
        if ("1" == selectedProcess) {
            // new Registration
            issueDt = document.getElementById('newRegRptIssueDt').value;
            lastDocRecDt = "";
            signature = document.getElementById('newRegAuthorityName').value;
        } else if ("2" == selectedProcess) {
            // Retention
            issueDt = document.getElementById('retentionRptIssueDt').value;
            lastDocRecDt = document.getElementById('retentionLastDocRecDt').value;
            signature = document.getElementById('retentionAuthorityName').value;
        } else if ("3" == selectedProcess) {
            // Restoration
            issueDt = document.getElementById('restorationRptIssueDt').value;
            lastDocRecDt = document.getElementById('restorationLastDocRecDt').value;
            signature = document.getElementById('restorationAuthorityName').value;
        } else if ("5" == selectedProcess) {
            // Removal
            issueDt = document.getElementById('removalRptIssueDt').value;
            lastDocRecDt = document.getElementById('removalLastDocRecDt').value;
            signature = document.getElementById('removalAuthorityName').value;
        }

        // check checkbox of Retension missingItem
        var missingItemRetensionChkList = document.getElementsByName("missingItem");
        var checked = "CHECKED";
        var missingItemRetensionNullChk = "";
        var missingItemRetensionChequeChk = "";
        var missingItemRetensionProfRegCertChk = "";
        var missingItemRetensionOthersChk = "";
        var missingItemRetensionIncompleteFormChk = "";
        for (i = 0; i < missingItemRetensionChkList.length; i++) {
            var missingItemChk = missingItemRetensionChkList[i];
            if (missingItemChk.checked) {
                if (i == 0) { missingItemRetensionNullChk = checked; }
                else if (i == 1) { missingItemRetensionChequeChk = checked; }
                else if (i == 2) { missingItemRetensionProfRegCertChk = checked; }
                else if (i == 3) { missingItemRetensionOthersChk = checked; }
                else if (i == 4) { missingItemRetensionIncompleteFormChk = checked; }
            }
        }

        // check checkbox of Restoration missingItem
        var missingItemRestorationChkList = document.getElementsByName("missingItem");
        var missingItemRestorationNullChk = "";
        var missingItemRestorationChequeChk = "";
        var missingItemRestorationProfRegCertChk = "";
        var missingItemRestorationOthersChk = "";
        var missingItemRestorationIncompleteFormChk = "";
        for (i = 0; i < missingItemRetensionChkList.length; i++) {
            var missingItemChk = missingItemRetensionChkList[i];
            if (missingItemChk.checked) {
                if (i == 0) { missingItemRetensionNullChk = checked; }
                else if (i == 1) { missingItemRestorationChequeChk = checked; }
                else if (i == 2) { missingItemRestorationProfRegCertChk = checked; }
                else if (i == 3) { missingItemRestorationOthersChk = checked; }
                else if (i == 4) { missingItemRestorationIncompleteFormChk = checked; }
            }
        }

        // default fileName
        var fileName = "AP_RSE.txt";

        // default url
        var targetUrl = '/Fn03PA_UAS/ExportCertificate';
        if ("CERT" == exportType) {
            targetUrl = '/Fn03PA_UAS/ExportCertificate';
            fileName = "AP_RSE.txt";
        } else if ("LETTER" == exportType) {
            targetUrl = '/Fn03PA_UAS/ExportLetter';
            fileName = "AP_RSE.txt";
        } else if ("LETTER_WITH_CODE" == exportType) {
            targetUrl = '/Fn03PA_UAS/ExportLetterWithCode';
            fileName = "AP_RSE.txt";
        } else if ("QP_CARD" == exportType) {
            targetUrl = '/Fn03PA_UAS/ExportQPCard';
            fileName = "QP_CARD.txt";
        } else {

        }
        downloadFile(targetUrl, {
                process: selectedProcess
                , certNo: certNo
                , indCertUuid: indCertUuid
                , rptIssueDt: issueDt
                , rptRcvdDt: lastDocRecDt
                , signature: signature
                , missingItemRetensionNullChk: missingItemRetensionNullChk
                , missingItemRetensionChequeChk: missingItemRetensionChequeChk
                , missingItemRetensionProfRegCertChk: missingItemRetensionProfRegCertChk
                , missingItemRetensionOthersChk: missingItemRetensionOthersChk
                , missingItemRetensionIncompleteFormChk: missingItemRetensionIncompleteFormChk
                , missingItemRestorationNullChk: missingItemRestorationNullChk
                , missingItemRestorationChequeChk: missingItemRestorationChequeChk
                , missingItemRestorationProfRegCertChk: missingItemRestorationProfRegCertChk
                , missingItemRestorationOthersChk: missingItemRestorationOthersChk
                , missingItemRestorationIncompleteFormChk: missingItemRestorationIncompleteFormChk
            });
        //$.ajax({
        //    type: "POST",
        //    url: targetUrl,
        //    //contentType: "charset=utf-8;",
        //    data: {
        //        process: selectedProcess
        //        , certNo: certNo
        //        , indCertUuid: indCertUuid
        //        , rptIssueDt: issueDt
        //        , rptRcvdDt: lastDocRecDt
        //        , signature: signature
        //        , missingItemRetensionNullChk: missingItemRetensionNullChk
        //        , missingItemRetensionChequeChk: missingItemRetensionChequeChk
        //        , missingItemRetensionProfRegCertChk: missingItemRetensionProfRegCertChk
        //        , missingItemRetensionOthersChk: missingItemRetensionOthersChk
        //        , missingItemRetensionIncompleteFormChk: missingItemRetensionIncompleteFormChk
        //        , missingItemRestorationNullChk: missingItemRestorationNullChk
        //        , missingItemRestorationChequeChk: missingItemRestorationChequeChk
        //        , missingItemRestorationProfRegCertChk: missingItemRestorationProfRegCertChk
        //        , missingItemRestorationOthersChk: missingItemRestorationOthersChk
        //        , missingItemRestorationIncompleteFormChk: missingItemRestorationIncompleteFormChk
        //    },
        //    success: function (data) {
        //        download(fileName, data);
        //    }
        //})
    }



    //calculater Date
        function calDate(id) {
            var postDate;
        var selectedProcess = document.getElementById("process");
        var selectedType = selectedProcess.options[selectedProcess.selectedIndex].text;

        if (selectedType == 'New Registration') {
            postDate = $('#C_IND_CERTIFICATE_REGISTRATION_DATE').val()
        }
        else if (selectedType == 'Retention') {
            postDate = $('#C_IND_CERTIFICATE_RETENTION_DATE').val()
        }
        else if (selectedType == 'Restoration') {
            postDate = $('#C_IND_CERTIFICATE_RESTORE_DATE').val()
        }

        document.getElementById("NewRegExtDate").value = "";
        document.getElementById("RetExtDate").value = "";
        document.getElementById("RetAppDate").value = "";
        document.getElementById("ResExtDate").value = "";
        document.getElementById("ResAppDate").value = "";

        $(".EditClass").html("");

        var NewRegValPerList = $("#NewRegVP option:selected").text();

        var RetValPerList = $("#RetVP option:selected").text();

        var ResValPerList = $("#ResVP option:selected").text();

        $.post("@Url.Action("CalculateIndDate","Fn03PA_UAS")", { id: id, selectedType: selectedType, registrationDate: postDate, NewRegValPerList: NewRegValPerList, RetValPerList: RetValPerList, ResValPerList: ResValPerList, code: document.getElementById("CatCode").value }
            , function (data) {
                if (selectedType =="New Registration") {
                    document.getElementById("NewRegExtDate").value = data.NEWREGEXTEND_DATE;
                }
                else if (selectedType =="Retention") {
                    document.getElementById("RetExtDate").value = data.RETEXTEND_DATE;
                    document.getElementById("RetAppDate").value = data.DateRetApp;
                }
                else if (selectedType =="Restoration") {
                    document.getElementById("ResExtDate").value = data.RESEXTEND_DATE;
                    document.getElementById("ResAppDate").value = data.DateResApp;
                }
                //else if (selectedType =="QP Card Application") {
                //    document.getElementById("QpExtDate").value = data.EXTEND_DATE;
                //}
            });
    }
    // download a file
    function download(filename, text) {
        var element = document.createElement('a');
        element.setAttribute('href', 'data:text/plain;charset=utf-8,' + decodeURIComponent(text));
        element.setAttribute('download', filename);
        element.style.display = 'none';
        document.body.appendChild(element);
        element.click();
        document.body.removeChild(element);
    }
    </script>
