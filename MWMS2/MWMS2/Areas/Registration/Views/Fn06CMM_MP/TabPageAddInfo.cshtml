@model MWMS2.Areas.Registration.Models.Fn06CMM_MPDisplayModel


<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.ENGLISH_CARE_OF) :</div>
        <div class="w3-col l9 m6 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.ENGLISH_CARE_OF)</div>
    </div>

    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.CHINESE_CARE_OF) :</div>
        <div class="w3-col l9 m6 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.CHINESE_CARE_OF)</div>
    </div>

    <div class="w3-col hr"></div>

    <div class="w3-col l6 m12 s12">
        <div class="w3-col l12 m12 s12 w3-center">Address @Html.LabelFor(o => o.C_COMMITTEE_MEMBER) <font color="red">*</font></div>
        <div class="w3-col l12 m12 s12 data">
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS.ADDRESS_LINE1)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS.ADDRESS_LINE2)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS.ADDRESS_LINE3)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS.ADDRESS_LINE4)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS.ADDRESS_LINE5)
        </div>
    </div>
    <div class="w3-col l6 m12 s12">
        <div class="w3-col l12 m12 s12 w3-center">Address @Html.LabelFor(o => o.C_COMMITTEE_MEMBER)</div>
        <div class="w3-col l12 m12 s12 data">
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS1.ADDRESS_LINE1)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS1.ADDRESS_LINE2)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS1.ADDRESS_LINE3)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS1.ADDRESS_LINE4)<br />
            @Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.C_ADDRESS1.ADDRESS_LINE5)
        </div>
    </div>


    <div class="w3-col hr"></div>


    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.TELEPHONE_NO1) <font color="red">*</font> :</div>
        <div class="w3-col l3 m2 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.TELEPHONE_NO1)</div>
        <div class="w3-col l3 m2 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.TELEPHONE_NO2)</div>
        <div class="w3-col l3 m2 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.TELEPHONE_NO3)</div>
    </div>

    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.FAX_NO1) :</div>
        <div class="w3-col l3 m3 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.FAX_NO1)</div>
        <div class="w3-col l3 m3 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.FAX_NO2)</div>
        <div class="w3-col l3 w3-hide-medium w3-hide-small data"></div>
    </div>

    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.EMAIL) :</div>
        <div class="w3-col l9 m6 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.EMAIL)</div>
    </div>

    <div class="w3-col hr"></div>


    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.RANK) :</div>
        <div class="w3-col l9 m6 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.RANK)</div>
    </div>

    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.POST) :</div>
        <div class="w3-col l9 m6 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.POST)</div>
    </div>

    <div class="w3-col l12 m12 s12">
        <div class="w3-col l3 m6 s12">@Html.LabelFor(o => o.C_COMMITTEE_MEMBER.CAREER) :</div>
        <div class="w3-col l9 m6 s12 data">@Html.TextBoxFor(o => o.C_COMMITTEE_MEMBER.CAREER)</div>
    </div>
</div>