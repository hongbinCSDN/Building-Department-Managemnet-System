                    <div class="w3-col l3 m6 s12">@Html.TextBoxFor(model => model.C_IND_PROCESS_MONITOR.RECEIVED_DATE, new { @id = "receivedIndDate", @class = "inputDate", @onblur = "CalDueDate(this,'" + Model.RegType + "')" })</div>
