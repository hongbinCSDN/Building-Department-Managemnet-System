                            @Html.CheckBoxFor(m => m.CategoryMWC, new { id = "MWC", name = "CRMWC", onclick = "checkboxCompany()" })
                            <label for="MWC">RMWC (Company)</label>
                            @Html.CheckBoxFor(m => m.CategoryMWI, new { id = "MWI", name = "IRMWC", onclick = "checkboxIndividual()" })
                            <label for="MWI">RMWC (Individual)</label>

                        </div>
                    </div>
                </div>
                <div class="w3-row">
                    <div class="w3-col l12 m12 s12">
