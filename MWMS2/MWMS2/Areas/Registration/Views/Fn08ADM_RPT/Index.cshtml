                            <label for="ALL">ALL</label>
                            @Html.CheckBoxFor(m => m.CategoryIP, new { id = "IP", name = "Professionals", onclick = "checkboxProfessional()" })
                            <label for="IP">Professionals (AP, RSE, RGE, RI)</label>
                            @Html.CheckBoxFor(m => m.CategoryGBC, new { id = "GBC", name = "Contractors", onclick = "checkbocContractor()" })
