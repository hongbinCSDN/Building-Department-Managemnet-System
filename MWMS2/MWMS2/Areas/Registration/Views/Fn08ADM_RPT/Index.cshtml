                        <div class="w3-col l9 m6 s12">
                            <div id="ALLTypeApp" class="TypeApp">
                                @foreach (var item in Model.ApplicationTypeList)
                                {
                                    string tmp = "";
                                    if (item.Value == "ALL")
                                    {
                                        tmp = "checked";
                                    }
                                    <input type="checkbox" value="@item.Key" id="@item.Key" class="@item.Value hide apItems" />
                                    @Html.Label(item.Key, new { @for = @item.Key, @class = @item.Value + " hide apItemLabels" })
                                }
                            </div>
                        </div>
                    </div>
                </div>
                <div class="w3-row">
                    <div class="w3-col l12 m12 s12">
