                                string tmp = "";
                                if (item.Value == "ALL")
                                {
                                    tmp = "checked";
                                }
                                <input type="checkbox" value="@item.Key" id="@item.Key" class="@item.Key hide irItems" />
                                @Html.Label(item.Value, new { @for = @item.Key, @class = @item.Key + " hide irItemLabels" })
                                @*string tmp = "";
                                    if (item.Selected == true)
