@model MWMS2.Areas.Registration.Models.CRMReportModel
<script type="text/javascript">
    function GeneratePnlInfoSheetForm(type)
    {
        if (type == "1") {
            attr("pnlInfoSheetReportId", "value", "CRM0032");
        }
        else {
            attr("pnlInfoSheetReportId", "value", "CRM0048");
        }

        openReport('pnlInfoSheetForm');
    }

    function GeneratePnlMonitorForm(type)
    {
        if (type == 'Company_Monitor_Reports')
            attr("pnlMonitorReportId", "value", "CRM0054");
        if (type == 'Applicants_Monitor') {
            if ($('#year').val() == null || $('#year').val() == '') {
                alert("Please input the year.");
                return;
            }
            attr("pnlMonitorReportId", "value", "CRM0055");
        }
        if (type == '10_Day_Pledge_Of_From_BA2')
            attr("pnlMonitorReportId", "value", "CRM0001");
        if (type == 'Monitor_On_Fast_Track_Processing')
            attr("pnlMonitorReportId", "value", "CRM0036");
        openReport('pnlMonitorForm');
    }

    function GeneratePnlAdminReportsForm(type)
    {

        if (type == 'Panel')
            attr("pnlAdminRptId", "value", "CRM0003");
            attr("pnlAdminCommittedTypeCode", "value", "CRC");
        if (type == 'GBC') {
            attr("pnlAdminRptId", "value", "CRM0002");
            attr("pnlAdminCommittedTypeCode", "value", "GBC");
        }
        if (type == 'SC') {
            attr("pnlAdminRptId", "value", "CRM0002");
            attr("pnlAdminCommittedTypeCode", "value", "SC");
        }
        openReport('pnlAdminReportsForm')
        
    }
</script>
