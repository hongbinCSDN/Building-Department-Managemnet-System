        } else if (source.value == 'MWC(P) - Class III') {
            document.getElementById("btnCSV").style.display = "none";
            list = list2;
            varList = varList2;
            textList = textList2;
        }
        target.innerHTML = '';
        for (var i = 0; i < list.length; i++) {

            var oOption = document.createElement("OPTION");
            oOption.value = list[i];
            if (i > 1) {
                oOption.innerHTML = "(" + list[i] + ")";
            } else {
                oOption.innerHTML = list[i];
            }
            target.appendChild(oOption);
        }
        $(divGroup).html("");
        if (varList.length > 0) {
            $(divGroup).show();
            console.log("***");
            console.log(divGroup.id);
            
            $(divGroup).append('<input value="" type="checkbox" class="checkAll ' + divGroup.id+'" onclick="checkAll(this)" />-Check All-');
        } else {

            $(divGroup).hide();
        }
        for (var i = 0; i < varList.length; i++) {
            var cCheckBox = document.createElement("INPUT");
            cCheckBox.type = "checkbox";
            cCheckBox.value = varList[i];

            cCheckBox.onclick = function () {
                chkCheck(this);
            };

            $(divGroup).append('<br/>');
            $(divGroup).append(cCheckBox);
            $(divGroup).append(textList[i]);
        }
    }
