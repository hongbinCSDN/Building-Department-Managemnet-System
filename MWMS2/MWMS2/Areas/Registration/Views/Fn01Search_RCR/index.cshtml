    function chkCheck(_this)
    {

      
        var i = 0;
        $(_this).parent().find(":checked").not(".checkAll").each(function () {
            i++;
        });

        if (i == $(_this).parent().find(":checkbox").not(".checkAll").length) {
            console.log($(_this).parent().find(".checkAll"));
            $(_this).parent().find(".checkAll").prop("checked", true)

        }
        else
        {
            $(_this).parent().find(".checkAll").prop("checked", false)
        }

    }


    function checkAll(_this) {
   
        var res = _this.className.split(" ");
        console.log(res);
        console.log(res[1]);
       // console.log($("."+res[1]).attr("checked"));
        console.log($('.'+res[1]+':checked').val());
        var checked = $('.' + res[1] + ':checked').val() == undefined ? false : true;
        //var checked = $(_this).attr("checked") == undefined ? false : true;
        $(_this).parent().find(":checkbox").not(".checkAll").prop("checked", checked);
    }
