    function changeList(source, target, divGroup) {
        var list;
        var varList;
        var textList;
        if (false) {
        } else if (source.value == '') {
            document.getElementById("btnCSV").style.display = "none";
            list = list0;
            varList = varList0;
            textList = textList0;
        } else if (source.value == 'GBC') {
            document.getElementById("btnCSV").style.display = "";
            list = list0;
            varList = varList0;
            textList = textList0;
        } else if (source.value == 'SC') {
            document.getElementById("btnCSV").style.display = "none";
            list = list1;
            varList = varList1;
            textList = textList1;
        } else if (source.value == 'MWC - Class I, II, III') {
            document.getElementById("btnCSV").style.display = "";
            list = list2;
            varList = varList2;
            textList = textList2;
        } else if (source.value == 'MWC - Class II, III') {
            document.getElementById("btnCSV").style.display = "";
