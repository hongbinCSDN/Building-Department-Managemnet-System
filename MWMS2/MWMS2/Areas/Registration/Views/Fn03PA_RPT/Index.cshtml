    <script type="text/javascript">

        function GeneratePnlCheckPRSForm() {
            countForQualification = 0;
            if (!$('#checkBoxCatPRSForm_0').is(':checked') && !$('#checkBoxCatPRSForm_1').is(':checked')
                && !$('#checkBoxCatPRSForm_2').is(':checked') && !$('#checkBoxCatPRSForm_3').is(':checked')
                && !$('#checkBoxCatPRSForm_4').is(':checked') && !$('#checkBoxCatPRSForm_5').is(':checked')
                && !$('#checkBoxCatPRSForm_6').is(':checked') && !$('#checkBoxCatPRSForm_6').is(':checked')) {
                alert("Please select at least one Status of Registration.");
                return;
            }

            if (!$('#checkBoxprbPRSForm_0').is(':checked') && !$('#checkBoxprbPRSForm_1').is(':checked')
                && !$('#checkBoxprbPRSForm_2').is(':checked') && !$('#checkBoxprbPRSForm_3').is(':checked')
                && !$('#checkBoxprbPRSForm_4').is(':checked')) {
                alert("Please select at least one Qualification.");
                return;
            }


            for (i = 0; i < 5; i++) {
                if ($('#checkBoxprbPRSForm_' + i).is(':checked'))
                    countForQualification += 1;
            }
            if (countForQualification > 3) {
                alert("You can only choose three qualification at most.");
                return;

            }

            openReport('pnlCheckPRSForm')
        }
    </script>
