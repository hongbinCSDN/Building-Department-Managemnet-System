    function showReportForm(divID) {
        var sDivs = $(".reportSelection").get();
        for (var i = 0; i < sDivs.length; i++) {
            if (sDivs[i].id == divID) {
                sDivs[i].style.display = "";
            } else {
                sDivs[i].style.display = "none";
            }
        }
    }
    //Checking the Professional Registration Status
    function updatePRSFormText() {
        document.getElementById('PrsHeader').value = '';
        document.getElementById('Short_name').value = '';
        var i = 0;
        while (document.getElementById('checkBoxCatPRSForm' + '_' + i) != undefined) {
            if (document.getElementById('checkBoxCatPRSForm' + '_' + i).checked) {
                if (document.getElementById('PrsHeader').value == '') {
                    document.getElementById('PrsHeader').value =
                        document.getElementById('PrsHeader').value +
                        document.getElementById('displayCatPRSForm' + '_' + i).value;

                } else {
                    document.getElementById('PrsHeader').value =
                        document.getElementById('PrsHeader').value + ' and ' +
                        document.getElementById('displayCatPRSForm' + '_' + i).value;
                }
                if (document.getElementById('Short_name').value != '' && i != 3) {
                    document.getElementById('Short_name').value += ' and ';
                }
                if (i == 3) {
                    if ($("#checkBoxprbPRSForm_3").is(":checked")) {
                        if ($("#checkBoxprbPRSForm_3").is(":checked") && document.getElementById('Short_name').value != '') {
                            document.getElementById('Short_name').value += ' and ';
                        }
                        document.getElementById('Short_name').value += document.getElementById('displayTitle_3_1').value;
                    }
                    if ($("#checkBoxprbPRSForm_4").is(":checked")) {
                        if ($("#checkBoxprbPRSForm_4").is(":checked") && document.getElementById('Short_name').value != '') {
                            document.getElementById('Short_name').value += ' and ';
                        }
                        document.getElementById('Short_name').value += document.getElementById('displayTitle_3_2').value;
                    }
                } else {
                    document.getElementById('Short_name').value += document.getElementById('displayTitle' + '_' + i).value;
                }
            }
            i++;
        }
    }
