<div class="displayForm">
    <div class="w3-container footer">
        <button id="preBtn" class="btn btn-default w3-left" type="button">Previous</button>
        <button id="cancelBtn" class="btn btn-default" type="button">Cancel</button>
    </div>
</div>

<script type="text/javascript">

    domReady(function () {
        var previous
            , cancel;

        previous = function () {
            goTo(domId("rootPath").value + '/MWProcessing/Fn02MWUR_GE/EnquiryFormReadOnly', [{ name: "id", value: "@Model.P_MW_GENERAL_RECORD.UUID" }]);
        };
        cancel = function () {
            goTo(domId("rootPath").value + '/MWProcessing/Fn03TSK_GS/Index');
        }

        attr("preBtn", "onclick", { parameters: {}, callback: previous });
        attr("cancelBtn", "onclick", { parameters: {}, callback: cancel });

    });

    function getDSN(eleId) {
        $.post("/MWProcessing/Fn02MWUR_GE/GeneralMWNumber", $("#inputForm").serialize(), function (data) {
            attr("inputForm", "dropClass", "saving");
            if (data != null && data.Result != null && data.Result == "SUCCESS") {
                console.log(data);
                $("#" + eleId).text(data.Data.MW_NUMBER);
            }
            else {
                showErrorMessage(data.ErrorMessages);
                console.log(data);
            }
        }, "json");
    }

</script>