    <div class="w3-row">
        <div style="float:right">
            <button class="searchButton btn btn-default" type="button"><i class="fa fa-search"></i> Search</button>
        </div>
    </div>

</div>
<div id="resultPanel2"></div>

<script type="text/javascript">
    domReady(function () {
        new searcher({
            searchPath: "SearchPart1"
            , exportPath: "Excel"
            , searchTable: "searchCriteria"
            , resultPanel: "resultPanel"
        });

        new searcher({
            searchPath: "SearchPart2"
            , exportPath: "Excel"
            , searchTable: "searchCriteria2"
            , resultPanel: "resultPanel2"
        });
    });
</script>
