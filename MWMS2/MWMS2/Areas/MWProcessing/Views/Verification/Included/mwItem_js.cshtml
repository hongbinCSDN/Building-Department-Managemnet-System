    function RemoveAddressRow(obj) {

        var index = obj.parentNode.parentNode.rowIndex - 1;
        var tb = document.getElementById('AddressTable');

        //get template data
        var AddressInfo = {};

        //get Record ID
        var Record = document.getElementsByName('P_MW_RECORD.UUID')[0];
        AddressInfo[Record.name] = Record.value;

        var cell = tb.tBodies[0].rows[index].cells[2];

        currentRow = cell.querySelectorAll("input[name^='P_MW_ADDRESSes[']");

        for (var i = 0; i < currentRow.length; i++) {
            var name = currentRow[i].name.substring(currentRow[i].name.indexOf('.'));
            AddressInfo['P_MW_ADDRESS' + name] = currentRow[i].value;
        }

        attr("AddressTable", "addClass", "deleting");
        $.post("DeleteAddressInfo", AddressInfo, function (data) {
            attr("AddressTable", "dropClass", "deleting");
            if (data != null && data.Result != null && data.Result == "SUCCESS") {

                tb.deleteRow(index + 1);
            }
            else {
                showErrorMessage(data.ErrorMessages);
                console.log(data);
            }
        }, "json");

    }

    function AddAddressInfo() {

        //get template data
        var AddressInfo = {};

        //get Record ID
        var Record = document.getElementsByName('P_MW_RECORD.UUID')[0];
        AddressInfo[Record.name] = Record.value;

        addressTemplate = document.getElementById('AddressTemplate');
        var inputList = addressTemplate.querySelectorAll("input[name^='P_MW_ADDRESS.']");

        var isEmptyForm = true;

        for (var i = 0; i < inputList.length; i++) {
            if (inputList[i].value != undefined && inputList[i].value != null && inputList[i].value.trim() != "") {
                isEmptyForm = false;
            }

            AddressInfo[inputList[i].name] = inputList[i].value;
        }

        if (isEmptyForm) {
            alert('Address information cannot be all empty');
            return;
        }

        //console.log(AddressInfo);
        attr("AddressTemplate", "addClass", "saving");
        $.post("AddAddressInfo", AddressInfo, function (data) {
            attr("AddressTemplate", "dropClass", "saving");
            if (data != null && data.Result != null && data.Result == "SUCCESS") {

                AddAddressRow(data.Data);
                //goTo("Index", null);
            }
            else {
                showErrorMessage(data.ErrorMessages);
                console.log(data);
            }
        }, "json");
    }

    function AddAddressRow(obj) {
        //Html string
        var trHtml = "<td></td>"
            + "<td></td>"
            + "<td></td>"
            + "<td><button type='button' onclick='RemoveAddressRow(this)' class='btn btn-default'>Delete<buttong></td>";

        //get table
        var tb = document.getElementById('AddressTable');

        //get table rows count
        var index = tb.tBodies[0].rows.length;
        //add row
        tb.tBodies[0].insertRow(index);
        tb.tBodies[0].rows[index].innerHTML = trHtml;

        var streeName = "";
        var streetNo = "";
        var buildingName = "";
        var address = "";
        var floor = "";
        var flat = "";

        if (obj['STREE_NAME']) {
            streeName = obj['STREE_NAME'];
            address += streeName;
        }

        if (obj['STREET_NO']) {

            if (address != "") {
                address += " , "
            }
            streetNo = obj['STREET_NO'];
            address += streetNo;
        }

        if (obj['BUILDING_NAME']) {
            if (address != "") {
                address += " , "
            }
            buildingName = obj['BUILDING_NAME'];
            address += buildingName;
        }

        if (obj['FLOOR']) {
            floor = obj['FLOOR'];
        }

        if (obj['FLAT']) {
            flat = obj['FLAT'];
        }


        tb.tBodies[0].rows[index].cells[0].innerText = address;
        tb.tBodies[0].rows[index].cells[1].innerText = floor;
        //tb.tBodies[0].rows[index].cells[2].innerText = obj['FLAT'];

        var hiddenHtml = "";

        var UUIDHtml = flat + "<input name='P_MW_ADDRESSes[" + index + "].UUID' type='text' value='" + obj['UUID'] + "' style='display:none;' />";
        hiddenHtml += UUIDHtml;

        tb.tBodies[0].rows[index].cells[2].innerHTML = hiddenHtml;
    }

    function disabledQuestion(uuid, checkedValue) {
        if (checkedValue == 'Y') {
            showQuestion(uuid);
        } else {

            $('.' + uuid).show();
            rootElement = document.getElementById(uuid);
            radioY = document.getElementById(uuid + "Y");
            radioN = document.getElementById(uuid + "N");
            radioM = document.getElementById(uuid + "M");
            radioRMK = document.getElementById(uuid + "_rmk");
            if (rootElement != null) {
                rootElement.disabled = true;
                rootElement.style.display = "inline";
            }
            if (radioY != null) {
                radioY.disabled = true;
                radioY.checked = false;
                radioY.style.display = 'inline';
            }
            if (radioN != null) {
                radioN.disabled = true;
                radioN.checked = false;
                radioN.style.display = 'inline';
            }
            if (radioM != null) {
                radioM.disabled = true;
                radioM.checked = false;
                radioM.style.display = 'inline';
            }
            if (radioRMK != null) {
                radioRMK.disabled = true;
                radioRMK.value = "";
                radioRMK.style.display = 'inline';
            }

        }
    }

