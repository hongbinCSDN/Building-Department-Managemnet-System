        function autoSelect(UUID, checkItems) {
            var result = "";
            var disabledCount = 0;

            for (i = 0; i < checkItems.length; i++) {
                if (document.getElementById(checkItems[i] + 'Y').disabled) {
                    disabledCount++;
                } else {
                    result += getRadioValue(checkItems[i]);
                    if (result.length + disabledCount <= i) break;
                }
            }
            if (result.length + disabledCount == checkItems.length) {
                if (result.indexOf('N') >= 0 || result.indexOf('M') >= 0) {
                    document.getElementById(UUID + 'N').checked = true;
                    return true;
                } else if (result.indexOf('Y') >= 0) {
                    document.getElementById(UUID + 'Y').checked = true;
                    return true;
                } else {
                    return false;
                }
            }

        }
    }

