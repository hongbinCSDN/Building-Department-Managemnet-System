    function changeMatchItem(name, value) {
        //get item code
        var itemCodeName = name.substring(0, name.indexOf(".")) + ".MW_ITEM_CODE";

        var itemCode = document.getElementsByName(itemCodeName)[0].value;

        var itemCheckBox = document.querySelectorAll("input[class='" + itemCode + " matchItem main']")[0];
        if (itemCheckBox) {
            if (value == "Y") {
                itemCheckBox.removeAttribute("disabled");
            } else {
                itemCheckBox.setAttribute("disabled", "True");
                setMatchItem(false, itemCheckBox.name);
            }
        }

    }

    function changeChildItem(name, isDisabled) {

        var formCode = document.getElementById("P_MW_RECORD_S_FORM_TYPE_CODE").value;

        var itemCodeName = name.substring(0, name.indexOf(".")) + ".MW_ITEM_CODE";

        var itemCode = document.getElementsByName(itemCodeName)[0].value;

        var radioList = document.querySelectorAll("input[class*='" + itemCode + "_radio']");
        var ddlList = document.querySelectorAll("select[class*='" + itemCode + "_ddl']");
        var taList = document.querySelectorAll("textarea[class*='" + itemCode + "_ta']");
        var txtList = document.querySelectorAll("input[class*='" + itemCode + "_txt']");
        var tapoList = document.querySelectorAll("textarea[class*='" + itemCode + "_ta_po']");


        for (var i = 0; i < radioList.length; i++) {

            if (isDisabled) {
                if (radioList[i].value == "N") {
                    radioList[i].checked = true;
                }
            } else {
                radioList[i].checked = false;
            }


            radioList[i].disabled = isDisabled;
        }

        for (var i = 0; i < ddlList.length; i++) {
            ddlList[i].disabled = isDisabled;
        }

        for (var i = 0; i < taList.length; i++) {

            //disabled
            if (isDisabled) {
                if (formCode == "MW10") {
                    taList[i].value = "Previous item not exist.";
                } else {
                    taList[i].value = "Revised item not exist.";
                }
            } else {
                taList[i].value = "";
            }

            taList[i].disabled = isDisabled;
        }

        for (var i = 0; i < tapoList.length; i++) {

            //disabled
            if (isDisabled) {
                if (formCode == "MW10") {
                    tapoList[i].value = "Previous item not exist.";
                } else {
                    tapoList[i].value = "Revised item not exist.";
                }
            } else {
                tapoList[i].value = "";
            }

            tapoList[i].disabled = isDisabled;
        }

        for (var i = 0; i < txtList.length; i++) {
            txtList[i].disabled = isDisabled;
        }
    }


    function initMmItem() {
        //get Form Code
        var formCode = document.getElementById("P_MW_RECORD_S_FORM_TYPE_CODE").value;

        if (formCode == "MW02" || formCode == "MW04" || formCode == "MW10") {

            var variationList = document.querySelectorAll("input[name$='].VARIATION_DECLARED");
            for (var i = 0; i < variationList.length; i++) {
                if (variationList[i].checked && variationList[i].value == "N") {
                    changeChildItem(variationList[i].name, true);
                }

            }

        }
    }

    domReady(function () {
        initMmItem();
    });

</script>
