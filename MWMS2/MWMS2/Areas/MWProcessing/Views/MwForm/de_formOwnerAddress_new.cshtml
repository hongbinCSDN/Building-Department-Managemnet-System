@model MWMS2.Areas.MWProcessing.Models.Fn02MWUR_DeFormModel


@Html.HiddenFor(model => model.OwnerPersonContact.UUID)
@Html.HiddenFor(model => model.OwnerAddress.UUID)
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Chinese Name</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerPersonContact.NAME_CHINESE, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12" style="height: 218px;">English Name</div>
        <div class="w3-col l8 m6 s12">
            <div class="w3-col l12 m12 s12">
                @Html.TextBoxFor(model => model.OwnerPersonContact.NAME_ENGLISH, new { @class = "w3-col l6 m12 s12" })
            </div>
            <div class="w3-col l12 m12 s12">
                @Html.TextBoxFor(model => model.OwnerPersonContact.ID_NUMBER, new { @class = "w3-col l6 m12 s12" })
            </div>
            <div class="w3-col l12 m12 s12">
                @Html.RadioButtonFor(model => model.OwnerPersonContact.ID_TYPE, "1", new { @class = "w3-radio" }) HKID No.
                @Html.RadioButtonFor(model => model.OwnerPersonContact.ID_TYPE, "2", new { @class = "w3-radio" }) Passport No.
                @Html.RadioButtonFor(model => model.OwnerPersonContact.ID_TYPE, "3", new { @class = "w3-radio" }) Business Registration No.
            </div>
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l3 m12 s12">
                    @Html.RadioButtonFor(model => model.OwnerPersonContact.ID_TYPE, "4", new { @class = "w3-radio" }) Other (Please specify)
                </div>
                @Html.TextBoxFor(model => model.OwnerPersonContact.OTHER_ID_TYPE, new { @class = "w3-col l6 m12 s12" })
            </div>
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l2 m12 s12">Country of Issue</div>
                @Html.TextBoxFor(model => model.OwnerPersonContact.ID_ISSUE_COUNTRY, new { @class = "w3-col l6 m12 s12" })
            </div>

        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Correspondence Address</div>
        <div class="w3-col l8 m6 s12">
            @Html.CheckBoxFor(model => model.OwnerPersonContact.IS_ADDRESS_SAME_A1) Address same as Part1
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Street/Road/Village</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerAddress.DISPLAY_STREET, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Street Number</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerAddress.DISPLAY_STREET_NO, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Building/Estate</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerAddress.BUILDING_NAME, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Floor</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerAddress.FLOOR, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Flat/Room</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerAddress.FLAT, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12" style="height:83px;">District</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerAddress.DISTRICT, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
            <div class="w3-col l7 m12 s12">
                @Html.RadioButtonFor(model => model.OwnerAddress.REGION, "HK", new { @class = "w3-radio" }) Hong Kong
                @Html.RadioButtonFor(model => model.OwnerAddress.REGION, "KLW", new { @class = "w3-radio" }) Kowloon
                @Html.RadioButtonFor(model => model.OwnerAddress.REGION, "NT", new { @class = "w3-radio" }) New Territories
            </div>
            @*<input type="radio" name="districtID" value="1" />Hong Kong
                <input type="radio" name="districtID" value="2" />Kowloon
                <input type="radio" name="districtID" value="3" />New Territories*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">E-mail Address</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerPersonContact.EMAIL, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Fax No.</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerPersonContact.FAX_NO, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Contact No.</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.OwnerPersonContact.CONTACT_NO, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
