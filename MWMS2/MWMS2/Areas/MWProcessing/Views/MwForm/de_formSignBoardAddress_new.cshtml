@model MWMS2.Areas.MWProcessing.Models.Fn02MWUR_DeFormModel

@Html.HiddenFor(model => model.SignBoardPersonContact.UUID)
@Html.HiddenFor(model => model.SignBoardAddress.UUID)
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Chinese Name</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardPersonContact.NAME_CHINESE, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12" style="height:218px;">English Name</div>
        <div class="w3-col l8 m6 s12">
            <div class="w3-col l12 m12 s12">
                @Html.TextBoxFor(model => model.SignBoardPersonContact.NAME_ENGLISH, new { @class = "w3-col l6 m12 s12" })

            </div>
            <div class="w3-col l12 m12 s12">
                @Html.TextBoxFor(model => model.SignBoardPersonContact.ID_NUMBER, new { @class = "w3-col l6 m12 s12" })

            </div>
            <div class="w3-col l12 m12 s12">
                @Html.RadioButtonFor(model => model.SignBoardPersonContact.ID_TYPE, "1", new { @class = "w3-radio" }) HKID No.
                @Html.RadioButtonFor(model => model.SignBoardPersonContact.ID_TYPE, "2", new { @class = "w3-radio" }) Passport No.
                @Html.RadioButtonFor(model => model.SignBoardPersonContact.ID_TYPE, "3", new { @class = "w3-radio" }) Business Registration No.

            </div>
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l3 m12 s12">
                    @Html.RadioButtonFor(model => model.SignBoardPersonContact.ID_TYPE, "4", new { @class = "w3-radio" }) Other (Please specify)
                </div>

                @Html.TextBoxFor(model => model.SignBoardPersonContact.OTHER_ID_TYPE, new { @class = "w3-col l6 m12 s12" })

            </div>
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l2 m12 s12">
                    Country of Issue
                </div>

                @Html.TextBoxFor(model => model.SignBoardPersonContact.ID_ISSUE_COUNTRY, new { @class = "w3-col l6 m12 s12" })

            </div>
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Correspondence Address</div>
        <div class="w3-col l8 m6 s12">
            @Html.CheckBoxFor(model => model.SignBoardPersonContact.IS_ADDRESS_SAME_A1) Address same as Part1
            @Html.CheckBoxFor(model => model.SignBoardPersonContact.IS_ADDRESS_SAME_A4) Address same as Part4
            @*@Html.TextBoxFor(model => model.P_MW_PERSON_CONTACT_F.ADDRESS_SAME_A1)
                @Html.TextBoxFor(model => model.P_MW_PERSON_CONTACT_F.ADDRESS_SAME_A4)*@
            @*<input type="checkbox" /> Address same as Part 1
                <input type="checkbox" />Address same as Part 4*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Street/Road/Village</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardAddress.DISPLAY_STREET, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Street Number</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardAddress.DISPLAY_STREET_NO, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Building/Estate</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardAddress.BUILDING_NAME, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Floor</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardAddress.FLOOR, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Flat/Room</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardAddress.FLAT, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12" style="height:83px;">District</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardAddress.DISTRICT, new { @class = "w3-col l6 m12 s12" })
            <div class="w3-col l7 m12 s12">
                @Html.RadioButtonFor(model => model.SignBoardAddress.REGION, "HK", new { @class = "w3-radio" }) Hong Kong
                @Html.RadioButtonFor(model => model.SignBoardAddress.REGION, "KLW", new { @class = "w3-radio" }) Kowloon
                @Html.RadioButtonFor(model => model.SignBoardAddress.REGION, "NT", new { @class = "w3-radio" }) New Territories
                @*<input type="text" />*@

            </div>
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Fax No.</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardPersonContact.FAX_NO, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Contact No.</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardPersonContact.CONTACT_NO, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">E-mail Address</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardPersonContact.EMAIL, new { @class = "w3-col l6 m12 s12" })
            @*<input type="text" />*@
        </div>
    </div>
</div>
<div class="w3-row">
    <div class="w3-col l12 m12 s12">
        <div class="w3-col l4 m6 s12">Sigature Date</div>
        <div class="w3-col l8 m6 s12">
            @Html.TextBoxFor(model => model.SignBoardPersonContact.ENDORSEMENT_DATE, new { @class = "inputDate" })
        </div>
    </div>
</div>

