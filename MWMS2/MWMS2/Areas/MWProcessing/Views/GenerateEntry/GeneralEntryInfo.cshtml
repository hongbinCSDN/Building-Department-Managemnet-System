@model MWMS2.Areas.MWProcessing.Models.Fn02MWUR_GEModel

@Html.HiddenFor(m => m.DSN)
@Html.HiddenFor(m => m.P_MW_REFERENCE_NO.UUID)

<div class="panelHeader">Confirm Scanned Documents</div>
<div class="displayForm">
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l3 m12 s12">
                Document S/N
            </div>
            <div class="w3-col l3 m12 s12">
                @Model.DSN
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l3 m12 s12">
                MW No.
            </div>
            <div class="w3-col l3 m12 s12">
                @Model.P_MW_REFERENCE_NO.REFERENCE_NO
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l3 m12 s12">
                Form Language
            </div>
            <div class="w3-col l3 m12 s12">
                <label>
                    @Html.RadioButtonFor(m => m.Language.CODE, "EN", new { @class = "w3-radio" }) English &nbsp;
                </label>
                <label>
                    @Html.RadioButtonFor(m => m.Language.CODE, "ZH", new { @class = "w3-radio" }) Traditional Chinese &nbsp;
                </label>
            </div>
            <div class="w3-col l3 m12 s12">
                Public Folder
            </div>
            <div class="w3-col l3 m12 s12">

            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l3 m12 s12">
                Received Date
            </div>
            <div class="w3-col l3 m12 s12">
                @Html.TextBoxFor(m => m.P_MW_GENERAL_RECORD.RECEIVE_DATE, new { @class = "inputDate" })
            </div>
            <div class="w3-col l3 m12 s12">
                Private Folder
            </div>
            <div class="w3-col l3 m12 s12">

            </div>
        </div>
    </div>
</div>