    <input type="button" onclick='location.href="@Url.Action("WN","WN")"' value="Cancel" />*@
<input type="button" onclick='location.href="@Url.Action("WN", "WN")"' value="Cancel" />
<input type="button" onclick='location.href="@Url.Action("WNCreateConfirm", "WN", Model)"' value="Confirm" />
