<script>
    function DownloadFile(uuid) {
        goTo('DownloadFile', [{ name: "uuid", value: uuid }], true);
    }

    function openPhotoLibFile(uuid) {
        $.ajax({
            url: "viewPhotoLibImage"
            , type: "POST"
            , data: {
                uuid: uuid
            }
            , success: function (d) {
                if (d != null) {
                    window.open(d, null, "dialogWidth:700px; dialogHeight:400px; center:yes");
                }
            }
        });
    }
</script>