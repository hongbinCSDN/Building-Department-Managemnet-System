<div class="sList">

    <div class="w3-border displayForm">
        <div class="w3-row" id="validation">
            <div class="panelHeader">Validation</div>
            <table class="resultTable">
                <thead>
                    <tr>
                        <th>Form Code</th>
                        <th>Received Date</th>
                        <th>Expiry Date</th>
                        <th>Validation Result</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        if (Model.ValidationList != null && Model.ValidationList.Count() > 0)
                        {
                            foreach (var item in Model.ValidationList)
                            {
                                string result = "";
                                if (item.ValidationResult != null)
                                {
                                    if (item.ValidationResult.Equals("A"))
                                    {
                                        result = "Accept";
                                    }
                                    else if (item.ValidationResult.Equals("R"))
                                    {
                                        result = "Refuse";
                                    }
                                    else if (item.ValidationResult.Equals("CA"))
                                    {
                                        result = "Conditional Accept";
                                    }
                                }
                                <tr>
                                    <td>@item.FormCode</td>
                                    <td>@item.ReceivedDate</td>
                                    <td>@item.ExpiryDate</td>
                                    <td>@result</td>
                                </tr>
                            }
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
