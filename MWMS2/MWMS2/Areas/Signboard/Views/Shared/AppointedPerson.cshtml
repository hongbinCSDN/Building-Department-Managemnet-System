@model MWMS2.Areas.Signboard.Models.DataEntryDisplayModel
<div class="displayForm">
    <div class="panelHeader">
        <label>
            Authorized person
            <br />
        </label>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">Certificate of Registration No. :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.CERTIFICATION_NO, new { @maxlength = "30" })
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">Chinese Name :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.CHINESE_NAME, new { @maxlength = "500" })
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">English Name :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.ENGLISH_NAME, new { @maxlength = "500" })
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">Contact No. :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.CONTACT_NO, new { @maxlength = "50" })
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">Fax No. :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.FAX_NO, new { @maxlength = "50" })
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">Expiry Date :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.EXPIRY_DATE, new { @class = "inputDate" })
            </div>
        </div>
    </div>
    <div class="w3-row">
        <div class="w3-col l12 m12 s12">
            <div class="w3-col l4 m6 s12">Signature Date :</div>
            <div class="w3-col l4 m6 s12">
                @Html.TextBoxFor(model => model.Ap.SIGNATURE_DATE, new { @class = "inputDate" })
            </div>
        </div>
    </div>
</div>
