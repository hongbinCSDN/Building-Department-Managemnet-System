@model MWMS2.Areas.Signboard.Models.DataEntryDisplayModel
    <div class="displayForm">
        <div class="panelHeader">
            <label>
                Person who arranged the works (PAW)
                <br />
            </label>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Chinese Name :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.NAME_CHINESE, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">English Name :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.NAME_ENGLISH, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12"></div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.ID_NUMBER, new { @maxlength = "50" })
                    <br />
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12"><span hide-medium w3-hide-small"><br /><br /><br /><br />&nbsp;</span></div>
  
                <div class="w3-col l4 m6 s12">
                    @Html.RadioButtonFor(model => model.Paw.ID_TYPE, "1", new { @class = "w3-radio", @id = "paw_hkid" }) <label for="paw_hkid">HKID No.</label>
                    @Html.RadioButtonFor(model => model.Paw.ID_TYPE, "2", new { @class = "w3-radio", @id = "paw_passport" }) <label for="paw_passport">Passport No.</label>
                    <br />
                    @Html.RadioButtonFor(model => model.Paw.ID_TYPE, "3", new { @class = "w3-radio", @id = "paw_brno" }) <label for="paw_brno">Business Registration No.</label>
                    @Html.RadioButtonFor(model => model.Paw.ID_TYPE, "4", new { @class = "w3-radio", @id = "paw_crno" }) <label for="paw_crno">Company Registration No.</label>
                    @Html.RadioButtonFor(model => model.Paw.ID_TYPE, "5", new { @class = "w3-radio", @id = "paw_iorno" }) <label for="paw_iorno">I/O Registration No.</label>
                    <br />
                    Country of Issue
                    @Html.TextBoxFor(model => model.Paw.ID_ISSUE_COUNTRY)
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Correspondence Address :<span hide-medium w3-hide-small"><br /><br />&nbsp;</span></div>
                <div class="w3-col l4 m6 s12">
                    <input class="inputButton" type="button" value="Copy from Signboard Address" style="width:300px;" onclick="copySignboardAddressToPAW()" />
                    <br /><input class="inputButton" type="button" value="Copy from Signboard owner Address" style="width:360px;" onclick="copyOwnerToPAW()" />
                    <br /><input class="inputButton" type="button" value="Copy from  Owner’ Corporations Address" style="width:400px;" onclick="copyCorporationsAddressToPAW()" />
                    <br />
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Street/Road/Village :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.STREET, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Street Number :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.STREET_NO, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Building/Estate :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.BUILDINGNAME, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Flat/Room :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.FLAT, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Floor :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.FLOOR, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Block :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.BLOCK, new { @maxlength = "30" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">District :<span hide-medium w3-hide-small"><br />&nbsp;</span></div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.B_SV_ADDRESS.DISTRICT, new { @maxlength = "200" })
                    <br />
                    @Html.RadioButtonFor(model => model.Paw.B_SV_ADDRESS.REGION, "HK", new { @class = "w3-radio", @id = "paw_district_HK" }) <label for="paw_district_HK">Hong Kong</label>
                    @Html.RadioButtonFor(model => model.Paw.B_SV_ADDRESS.REGION, "KLW", new { @class = "w3-radio", @id = "paw_district_KLN" }) <label for="paw_district_KLN">Kowloon</label>
                    @Html.RadioButtonFor(model => model.Paw.B_SV_ADDRESS.REGION, "NT", new { @class = "w3-radio", @id = "paw_district_NT" }) <label for="paw_district_NT">New Territories</label>
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">E-mail Address :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.EMAIL, new { @maxlength = "100" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Contact No. :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.CONTACT_NO, new { @maxlength = "50" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Fax No. :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Paw.FAX_NO, new { @maxlength = "100" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">PAW Same As :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.DropDownListFor(model => model.Paw.PAW_SAME_AS, Model.PawSameAsList)
                </div>
            </div>
        </div>
    </div>