@model MWMS2.Areas.Signboard.Models.DataEntryDisplayModel
    <div class="displayForm">
        <div class="panelHeader">
            <label>
                招牌擁有人的詳情 Particulars of the Signboard owner
                <br />
            </label>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Chinese Name :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Owner.NAME_CHINESE, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">English Name :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Owner.NAME_ENGLISH, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12"></div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Owner.ID_NUMBER, new { @maxlength = "50" })
                    <br />
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12"><span class="w3-hide-medium w3-hide-small"><br><br /><br /><br />&nbsp;</span></div>
                <div class="w3-col l4 m6 s12">
                    @Html.RadioButtonFor(model => model.Owner.ID_TYPE, "1", new { @class = "w3-radio", @id = "owner_hkid" }) <label for="owner_hkid">HKID No.</label>
                    @Html.RadioButtonFor(model => model.Owner.ID_TYPE, "2", new { @class = "w3-radio", @id = "owner_passport" }) <label for="owner_passport">Passport No.</label>
                    <br />
                    @Html.RadioButtonFor(model => model.Owner.ID_TYPE, "3", new { @class = "w3-radio", @id = "owner_brno" }) <label for="owner_brno">Business Registration No.</label>
                    @Html.RadioButtonFor(model => model.Owner.ID_TYPE, "4", new { @class = "w3-radio", @id = "owner_crno" }) <label for="owner_crno">Company Registration No.</label>
                    @Html.RadioButtonFor(model => model.Owner.ID_TYPE, "5", new { @class = "w3-radio", @id = "owner_iorno" }) <label for="owner_iorno">I/O Registration No.</label>
                    <br />
                    Country of Issue
                    @Html.TextBoxFor(model => model.Owner.ID_ISSUE_COUNTRY, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Correspondence Address :</div>
                <div class="w3-col l4 m6 s12">
                    <input class="inputButton" type="button" value="Copy from Signboard Address" style="width:300px;" onclick="copySignboardAddressToOwner()" /><br />
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Street/Road/Village :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.STREET, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Street Number :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.STREET_NO, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Building/Estate :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.BUILDINGNAME, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Flat/Room :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.FLAT, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Floor :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.FLOOR, new { @maxlength = "500" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Block :</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.BLOCK, new { @maxlength = "30" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12" >District :<span class="w3-hide-medium w3-hide-small"><br />&nbsp;</span></div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.OwnerAddress.DISTRICT, new { @maxlength = "200" })
                    <br />
                    @Html.RadioButtonFor(model => model.OwnerAddress.REGION, "HK", new { @class = "w3-radio", @id = "owner_district_HK" }) <label for="owner_district_HK">Hong Kong</label>
                    @Html.RadioButtonFor(model => model.OwnerAddress.REGION, "KLW", new { @class = "w3-radio", @id = "owner_district_KLN" }) <label for="owner_district_KLN">Kowloon</label>
                    @Html.RadioButtonFor(model => model.OwnerAddress.REGION, "NT", new { @class = "w3-radio", @id = "owner_district_NT" }) <label for="owner_district_NT">New Territories</label>
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">E-mail Address</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Owner.EMAIL, new { @maxlength = "100" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Contact No.</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Owner.CONTACT_NO, new { @maxlength = "50" })
                </div>
            </div>
        </div>
        <div class="w3-row">
            <div class="w3-col l12 m12 s12">
                <div class="w3-col l4 m6 s12">Fax No.</div>
                <div class="w3-col l4 m6 s12">
                    @Html.TextBoxFor(model => model.Owner.FAX_NO, new { @maxlength = "100" })
                </div>
            </div>
        </div>
    </div>