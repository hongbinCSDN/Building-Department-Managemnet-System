@model IEnumerable<MWMS2.Entity.B_S_SYSTEM_VALUE>

<h3>Validation Item</h3>
<div class="btnSet btnSetTypeA">
    <div class="right">
        @*<input class="inputButton" id="" name="" value="Create Validation Item" onclick='location.href="@Url.Action("SPMValidationItemCreate", "SPMValidationItem")"' type="button">*@
        <button class="btn btn-default" id="" name="" value="Create Validation Item" onclick='location.href="@Url.Action("SPMValidationItemCreate", "SPMValidationItem")"' type="button"><i class="far fa-plus-square"></i> Create Validation Item</button>
    </div>
</div>
<br>
<div class="sList">
    <table class="resultTable">
        <thead>
            <tr>
                <th class="sTh" style="text-align: left; padding-left: 8px;">Validation Item</th>
                <th class="sTh" style="text-align: left; padding-left: 8px;">Corresponding MW Item</th>
                <th class="sTh" style="text-align: left; padding-left: 8px;">Ordering</th>
            </tr>
        </thead>
        <tbody>
            @{
                foreach (var item in Model)
                {
                    <tr>
                        <td>

                            @Html.ActionLink(item.CODE, "SPMValidationItemEdit", "SPMValidationItem", new { id = item.UUID }, null)
                            @*@Html.DisplayFor(modelItem => item.REGISTRATION_NO)*@
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DESCRIPTION)
                        </td>
                        <td>
                            @*@Html.DisplayFor(modelItem => item.ORDERING)*@
                            @((int)item.ORDERING)
                        </td>

                    </tr>
                }
            }
        </tbody>
    </table>
</div>
<br>
