

CREATE TABLE MWMS_BLK_FILEREF AS
SELECT 
F.ADR_BLK_FILEREF_ID, F.ADR_BLK_ID,  R.FREF_SEQ, R.FREF_YR
FROM ADR_BLK_FILEREF@BCISDBLINK F, SYS_FILE_REF@BCISDBLINK R 
WHERE F.SYS_FILE_REF_ID = R.SYS_FILE_REF_ID 



CREATE TABLE MWMS2.MWMS_BLK_FILEREF
	(
	ADR_BLK_FILEREF_ID NUMBER (20) NOT NULL,
	ADR_BLK_ID         NUMBER (20),
	FREF_SEQ           VARCHAR2 (4),
	FREF_YR            VARCHAR2 (2)
	)
	TABLESPACE USERS
	STORAGE (BUFFER_POOL DEFAULT);

