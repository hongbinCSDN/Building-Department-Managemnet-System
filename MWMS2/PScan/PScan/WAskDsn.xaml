<Window x:Class="PScan.WAskDsn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PScan"
        mc:Ignorable="d"
        Title="Merge Document" Height="130" Width="320">
    <WrapPanel >

        <TextBlock Text="DSN : " Width="300" Margin="10,10,0,0"/>
        <TextBox  Name="TextBoxDsn" Width="180" Margin="10,5,0,5"/>
        <Button Name="ButtonGen" Width="90" Margin="0,5,10,5" Content="Create DSN" Click="ButtonGen_Click"/>
        <Button Name="ButtonOK" Width="270" Margin="10,5,10,5" Content="OK" Click="ButtonOK_Click"/>
    </WrapPanel>
</Window>
